{
    "name": "gupalo/json",
    "description": "JSON serializer/deserializer",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.3"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gupalo\\Json\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\Tests\\": "tests/"
        }
    }
}
